{
  "name": "@joemaddalone/path",
  "version": "1.2.0",
  "description": "a simple svg path generation utility",
  "main": "dist/path.min.js",
  "module": "src/index.js",
  "browser": "dist/path.min.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "format": "prettier --write \"**/*.+(js|jsx|json)\"",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "rm -rf dist && npm test && npm run build",
    "predeploy": "cd demo && npm install && npx snowpack build",
    "deploy": "gh-pages -d demo/build"
  },
  "keywords": [
    "svg",
    "path",
    "arc",
    "quadratic curve",
    "cubic curve"
  ],
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joemaddalone/path.git"
  },
  "author": "Joe Maddalone",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joemaddalone/path/issues"
  },
  "homepage": "https://joemaddalone.github.io/path/",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-babel": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "rollup": "^2.26.11",
    "rollup-plugin-terser": "^7.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|json)": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {}
}
